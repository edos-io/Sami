{
    "name": "edos-io/sami",
    "type": "application",
    "description": "Sami, an API documentation generator",
    "keywords": ["phpdoc"],
    "homepage": "https://github.com/edos-io/Sami",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier with contributions by edos.io",
            "email": "jeremy@edos.io"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "pimple/pimple": "~3.0",
        "twig/twig": "~1.13",
        "nikic/php-parser": "~1.0",
        "michelf/php-markdown": "~1.3",
        "symfony/console": "~2.1",
        "symfony/finder": "~2.1",
        "symfony/filesystem": "~2.1",
        "symfony/yaml": "~2.1",
        "symfony/process": "~2.1",
        "phpdocumentor/reflection-docblock": "~2.0"
    },
    "autoload": {
        "psr-4": { "Sami\\": "Sami/" }
    },
      "packages": {
        "edos-io/sami": {
          "version": "@dev-master"
        }
    },
    "bin": ["sami.php"],
  "minimum-stability": "dev"
}
